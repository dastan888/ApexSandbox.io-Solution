public Id classFromCourse(String courseName) {
    // code here
    
    List<apxio__Course__c> courses = [SELECT Name FROM apxio__Course__c WHERE Name =:courseName];
    if(courses.size() > 0){
        apxio__Class__c clas = new apxio__Class__c(
            apxio__Course__c = course.Id
        );
        insert clas;
        return clas.Id;
    } else {
        return null;
    }
}